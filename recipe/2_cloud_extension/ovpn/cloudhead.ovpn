client
remote <sms_public_ip>
port 443
proto tcp
dev tun
dev-node <cloud_hpc_head_node_public_ip>
ns-cert-type server
reneg-sec 86400
comp-lzo yes
verb 3
ca /etc/openvpn/keys/ca.crt
cert /etc/openvpn/keys/cloudhead.crt
key /etc/openvpn/keys/cloudhead.key
