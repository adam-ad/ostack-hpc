#!../common/bats/bin/bats
# -*-sh-*-

load ../common/test_helper_functions || exit 1
source ../common/functions || exit 1

if [ -s ../TEST_ENV ];then
    source ../TEST_ENV
fi

if [ "$RESOURCE_MANAGER" = "slurm" ];then
    rm=slurm
else
    ERROR "Unsupported or unknown resource manager"
    exit 1
fi

@test "[nagios] check for RPM" {
    check_if_rpm_installed "nagios${DELIM}"
}

@test "[nagios] test check_http" {
    /usr/lib64/nagios/plugins/check_http -H localhost -u /WW/static/test.html
}
